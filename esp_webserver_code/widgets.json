
{
    "powerButton": {
        "name": "powerButton",
        "sizeLimits": {"MinWidth": 1, "MaxWidth": 6, "MinHeight": 1, "MaxHeight": 6},
        "widgetStructure": "?",

        "dependentVars": {"printerPowerStatus": "printerPowerStatus"},
        "shortenedValues": [{"full": "'powerRed.svg'", "short": "powerRed"}, {"full": "'powerGreen.svg'", "short": "powerGreen"}],
        "jsSetup": "function(){if(printerPowerStatus == 1){$/$replaceBackgroundImage$/$($powerRed, $powerGreen); break;} if(printerPowerStatus == 0){$/$replaceBackgroundImage$/$($powerGreen, $powerRed); break;}}",
        "jsFunction": 1,
        "jsUnsetup": 1,
        "divHTML": {"att":"onclick=", "code":"$/$runCode$/$(($/$name$/$))"}

    }
}
